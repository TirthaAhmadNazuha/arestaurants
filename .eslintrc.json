{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "no-alert": "off",
        "linebreak-style": "off",
        "class-methods-use-this": "off",
        "no-restricted-syntax": "off",
        "no-useless-constructor": "off",
        "no-underscore-dangle": "off",
        "prefer-template": "off",
        "consistent-return": "off",
        "dot-notation": "off",
        "no-param-reassign": "off",
        "no-plusplus": "off",
        "prefer-destructuring": "off",
        "no-restricted-globals": "off",
        "import/no-extraneous-dependencies": "off",
        "import/order": "off",
        "eqeqeq": "off"
    }
}